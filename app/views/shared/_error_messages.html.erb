<% if object.errors.any? %>
  <div id ="error explanation">
    <div id ="alert alert-danger">
      This form contains <%=  pluralize(object.errors.count, "errors") %>
    </div>
  </div>
 <ul>
  <% object.errors.full_messages.each do |e| %>
  <li><%= e %></li>
  <% end %>
 </ul>
 </div> 
<% end %>  
